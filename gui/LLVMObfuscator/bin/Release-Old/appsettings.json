{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "FilePath": "Logs/llvm-obfuscator-.log",
    "RetainedFileCountLimit": 30,
    "FileSizeLimitBytes": 10485760
  },
  "Application": {
    "Version": "4.0.0",
    "Name": "LLVM Code Obfuscator",
    "MaxRecentFiles": 10,
    "AutoSave": true,
    "AutoSaveInterval": 300,
    "CheckForUpdates": true,
    "Theme": "Light"
  },
  "Obfuscation": {
    "DefaultOutputSuffix": "_obfuscated",
    "DefaultReportSuffix": "_report",
    "MaxFileSize": 52428800,
    "Timeout": 300,
    "DefaultPreset": "Standard"
  },
  "UI": {
    "WindowWidth": 1400,
    "WindowHeight": 900,
    "MinWindowWidth": 1200,
    "MinWindowHeight": 800,
    "EnableAnimations": true,
    "ShowTooltips": true
  }
}

